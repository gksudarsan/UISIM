	public void regularClaim49560(WebDriver driver, String snapshotPath, String SimpleClassname, String SSN) throws InterruptedException
	{
		wcl = new Wc_locators();
		f = new FutureDatePastDate();
		cit = new claimsIntake();
		driver.manage().timeouts().implicitlyWait(30, TimeUnit.SECONDS);
		driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		wcl.homeTabs("Unemployment Claim").click(); Thread.sleep(2500);
		driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		wcl.homeSubTabs("File Unemployment Claim").click(); Thread.sleep(2500);
		driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		js = ((JavascriptExecutor)driver);
		js.executeScript("window.scrollTo(0, document.body.scrollHeight)"); Thread.sleep(500);
		driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		wcl.clickNext().click(); Thread.sleep(2500);
		driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		wcl.ssn1().sendKeys(SSN); Thread.sleep(500);
		wcl.confirmssn1().sendKeys(SSN); Thread.sleep(500);
		driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		wcl.clickNext().click(); Thread.sleep(2500);
		driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);

		//Personal Information...
		cit.first_name().sendKeys(new RandomGenerator().randomStringGenerator(6)); Thread.sleep(500);
		cit.last_name().sendKeys(new RandomGenerator().randomStringGenerator(6)); Thread.sleep(500);
		cit.enterDate("dateOfBirth").sendKeys(f.d(-14600)); Thread.sleep(500);
		cit.randomGender(String.valueOf(new Random().nextInt(2))); Thread.sleep(500);
		cit.races(String.valueOf((int)(Math.random()*6)+1)); Thread.sleep(500);
		cit.ethinicity().click(); Thread.sleep(500);
		cit.us_citizen_yes().click(); Thread.sleep(500);
		driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		cit.military_vetern().click(); Thread.sleep(500);
		cit.highestGrade(String.valueOf((int)(Math.random()*26)+1)); Thread.sleep(500);
		cit.language_interpreter().click(); Thread.sleep(500);
		cit.addDependents("Child"); Thread.sleep(500);
		driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		wcl.clickNext().click(); Thread.sleep(2500);
		driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);

		//Dependent Child Information...
		String claimantWeeklyIncome = String.valueOf((int)(Math.random()*10)+1);
		String spouseWeeklyIncome = String.valueOf((int)(Math.random()*10)+1);
		for(int w = 1 ; w <= 5; w++)
		{
			cit.childFirstName(new RandomGenerator().randomStringGenerator(6)); Thread.sleep(500);
			cit.childLastName(new RandomGenerator().randomStringGenerator(6)); Thread.sleep(500);
			cit.enterDate("childDateOfBirth").sendKeys(f.d(-3650-(w*365))); Thread.sleep(500);
			cit.isChildSameHousehold("Yes"); Thread.sleep(500);
			cit.hasDisability("No"); Thread.sleep(500);
			cit.claimantWeeklyIncome(claimantWeeklyIncome); Thread.sleep(500);
			cit.householdCount("7"); Thread.sleep(500);
			driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);
			cit.spouseWeeklyIncome(spouseWeeklyIncome); Thread.sleep(500);
			cit.isReceivingChildSupport("No"); Thread.sleep(500);
			driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);
			wcl.clickNext().click(); Thread.sleep(2500);
			driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);

			//List of Dependents...
			if(w < 5)
			{
				cit.addAnotherChild("Yes"); Thread.sleep(500);
				driver = screen.getScreenShot(driver, snapshotPath, SimpleClassname);
				wcl.clickNext().click(); Thread.sleep(2500);
			}
		}		
	}

