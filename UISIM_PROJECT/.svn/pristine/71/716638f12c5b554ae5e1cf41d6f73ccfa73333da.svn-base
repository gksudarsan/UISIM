			pl.Date("allowStartDate").sendKeys(FutureDatePastDate.addDaysToDate(claimWeekEndingDate, -15)); Thread.sleep(500);
			pl.allowReason(); Thread.sleep(500);
		}
		else if(adjudicationDecision.equalsIgnoreCase("Deny"))
		{
			pl.Date("denyStartDate").sendKeys(FutureDatePastDate.addDaysToDate(claimWeekEndingDate, -15)); Thread.sleep(500);
			pl.overpaymentType("Non-Fraud"); Thread.sleep(500);
			pl.waiverApproval("Yes"); Thread.sleep(500);
			pl.remarks();
		}
		pl.isChargeOnly("No"); Thread.sleep(500);
		driver=screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		wcl.clickNext().click();
		Thread.sleep(5000);

		heading = wcl.heading();
		if(heading.equalsIgnoreCase("Decision Verification"))
			Thread.sleep(1000);
		else
		{
			System.out.println("Waiting for 15 more seconds for page to load...");
			Thread.sleep(15000);
		}
		driver=screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		js.executeScript("window.scrollTo(0, document.body.scrollHeight)"); Thread.sleep(500);
		driver=screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		wcl.submit().click();
		Thread.sleep(5000);
		driver=screen.getScreenShot(driver, snapshotPath, SimpleClassname);
	}
}