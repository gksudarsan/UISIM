		Random r = new Random();
		int result = r.nextInt(count-1) + 1;
		for(int i = 1; i <= count; i++)
		{
			String xpath = "//td[contains(text(), 'SSN')]//ancestor::table/tbody/tr["+i+"]";
			if(i==result)
			{	
				details[1] = driver.findElement(By.xpath(xpath+"//child::td/input[contains(@id, 'Ssn')]")).getAttribute("value");
				driver.findElement(By.xpath(xpath+"//child::td/label[contains(@for, 'percentageReduction:0')]/../div")).click(); Thread.sleep(500);
				driver.findElement(By.xpath(xpath+"//child::td/input[contains(@id, 'percentageReductionValue')]")).sendKeys("12"); Thread.sleep(500);
				driver.findElement(By.xpath(xpath+"//child::td//child::label[contains(text(), '-Select-')]")).click(); Thread.sleep(500);
				wcl.SelectEmploymentStatus("//ul[contains(@id, '"+(i-1)+":employeeworkshareweeklycertdetails_-reason_items')]/li[contains(text(), 'Lack of work / Laid off')]").click(); Thread.sleep(500);
				driver.findElement(By.xpath(xpath+"//child::td/label[contains(@for, 'hoursWorked:0')]/../div")).click(); Thread.sleep(500);
				driver=screen.getScreenShot(driver, snapshotPath, SimpleClassname);
			}

			else
				driver.findElement(By.xpath(xpath+"//child::td/label[contains(@for, 'percentageReduction:1')]/../div")).click();

			driver.findElement(By.xpath(xpath+"//child::td/input[contains(@id, '-comment')]")).sendKeys("Testing Record "+i); Thread.sleep(500);
		}
		System.out.println("The SSN on which weekly certification should be filed : "+details[1]);
		driver=screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		wcl.clickNext().click();
		Thread.sleep(5000);

		//Submission of Shared Work Employer Weekly Certification - Verification Certification page...
		if(heading.equalsIgnoreCase("Shared Work Employer Weekly Certification - Verification Certification"))
			Thread.sleep(1000);
		else
		{
			System.out.println("Waiting for 15 more seconds for page to load...");
			Thread.sleep(15000);
		}
		driver=screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		wcl.submit().click();
		Thread.sleep(5000);
		driver=screen.getScreenShot(driver, snapshotPath, SimpleClassname);
		details[0] = details[0].replaceAll("/", "");
		return details;
	}

