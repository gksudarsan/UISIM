		wcl.clickNext().click();
		Thread.sleep(5000);

		//Submission of Shared Work Employer Weekly Certification - File Certification page...
		heading = wcl.heading();
		while(heading.equalsIgnoreCase("Shared Work Employer Weekly Certification - Verification Certification"))
		{
			counter++;
			wcl.clickBack().click();
			Thread.sleep(5000);
			driver.findElement(By.xpath("//b[contains(text(), 'Week(s) to be Certified')]//ancestor::table//following::table[1]/tbody/tr["+(count-counter)+"]//following::div[1]/span")).click();
			Thread.sleep(500);
			details[0] = driver.findElement(By.xpath("//b[contains(text(), 'Week(s) to be Certified')]//ancestor::table//following::table[1]/tbody/tr["+(count-counter)+"]/td[2]/input")).getAttribute("value");
